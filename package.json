    "plop": "plop",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,css,scss}\"",
    "prettier:write": "prettier --write \"src/**/*.{js,jsx,css,scss}\"",
    "eject": "react-scripts eject"
  },
