  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build": "env-cmd -f .env.development react-scripts build",
    "rebuild": "env-cmd -f .env.development react-scripts build && (mv builded old_build || true) && mv build builded && rm -rf old_build",
    "rebuild:prod": "env-cmd -f .env.production react-scripts build && (mv builded old_build || true) && mv build builded && rm -rf old_build",
